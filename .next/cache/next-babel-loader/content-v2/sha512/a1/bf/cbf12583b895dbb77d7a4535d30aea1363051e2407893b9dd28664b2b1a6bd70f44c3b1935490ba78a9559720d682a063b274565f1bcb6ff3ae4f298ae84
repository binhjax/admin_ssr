{"ast":null,"code":"import { PortModel } from '@projectstorm/react-diagrams';\nimport { CustomLinkModel } from './CustomLinkModel';\nimport v4 from 'uuid';\nexport class CustomPortModel extends PortModel {\n  constructor(name, type, key) {\n    super({\n      type: 'custom',\n      name: v4(),\n      extras: {\n        name: name,\n        type: type,\n        key: key,\n        hover: ''\n      }\n    });\n  }\n\n  setHover(value) {\n    this.getOptions().extras.hover = value;\n    Object.values(this.getLinks()).forEach(link => {\n      if (link.isSelected() !== value) {\n        link.setSelected(value);\n\n        if (link.getSourcePort) {\n          if (link.getSourcePort().getOptions().extras.hover !== value) {\n            link.getSourcePort().setHover(value);\n          }\n        }\n\n        if (link.getTargetPort != null) {\n          console.log(link.getTargetPort());\n\n          if (link.getTargetPort().getOptions().extras.hover !== value) {\n            link.getTargetPort().setHover(value);\n          }\n        }\n      }\n    });\n  }\n\n  link(target) {\n    let link = new CustomLinkModel();\n    link.setSourcePort(this);\n    link.setTargetPort(target);\n    return link;\n  }\n\n  createLinkModel() {\n    let link = new CustomLinkModel();\n    link.registerListener({});\n    return link;\n  }\n\n}","map":{"version":3,"sources":["/home/tuanquang/admin_ssr/src/components/Test/CustomPortModel.ts"],"names":["PortModel","CustomLinkModel","v4","CustomPortModel","constructor","name","type","key","extras","hover","setHover","value","getOptions","Object","values","getLinks","forEach","link","isSelected","setSelected","getSourcePort","getTargetPort","console","log","target","setSourcePort","setTargetPort","createLinkModel","registerListener"],"mappings":"AAAA,SAASA,SAAT,QAA0B,8BAA1B;AACA,SAASC,eAAT,QAAgC,mBAAhC;AACA,OAAOC,EAAP,MAAe,MAAf;AACA,OAAO,MAAMC,eAAN,SAA8BH,SAA9B,CAAwC;AAC7CI,EAAAA,WAAW,CAACC,IAAD,EAAeC,IAAf,EAA6BC,GAA7B,EAA0C;AACnD,UAAM;AACJD,MAAAA,IA